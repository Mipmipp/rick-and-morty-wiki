{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "import/extensions": {
        "js": "always"
      },
    "rules": {
        "linebreak-style": 0,
        "indent": "off",
        "no-console": "off",
        "no-unused-vars": "off",
        "no-plusplus": "off",
        "no-useless-return": "off"
    }
}
